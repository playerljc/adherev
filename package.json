  "private": true,
  "description": "一个基于Vue的通用组件库，这个是在平时做的项目中沉淀出来的公共组件，包括UI、功能和其他",
  "bugs": {
    "url": "http://git.baifendian.com/SYYF/adherev/issues"
  },
  "repository": {
    "type": "git",
    "url": "http://git.baifendian.com/SYYF/adherev"
  },
  "license": "ISC",
  "author": "playerljc <playerljc@163.com>",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "buildpackages": "lerna run buildpackage",
    "buildumd": "node scripts/buildumd.js",
    "changed": "lerna changed",
    "clean": "lerna clean",
    "generatorchangelog": "lerna run generatorchangelog",
    "link": "lerna link",
    "lint": "eslint -c ./.eslintrc.js --fix --ext .jsx,.js,.vue,.ts,.tsx ./packages",
    "lint:fix": "eslint --fix  -c ./.eslintrc.js --fix --ext .jsx,.js,.vue,.ts,.tsx ./packages",
    "lint:style": "stylelint \"{site,packages}/**/*.less\" --syntax less",
    "list": "lerna list",
    "prettier": "prettier -c --write '**/*'",
    "pretty-quick": "pretty-quick",
    "publish": "lerna publish",
    "publish:alpha": "lerna publish --dist-tag alpha",
    "publish:beta": "lerna publish --dist-tag beta",
    "publish:next": "lerna publish --dist-tag next"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.0.0",
