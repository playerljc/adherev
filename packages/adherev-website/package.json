{
  "name": "@baifendian/adherev-website",
  "version": "1.0.0",
  "private": true,
  "description": "adherevçš„website",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "startapp": "cross-env environment=dev mode=development ctbuildv startapp --define alias=@,evnVars=true,cssModules=true,static=assets",
    "buildapp": "cross-env environment=dev mode=production ctbuildv buildapp --define alias=@,evnVars=true,cssModules=true,static=assets",
    "lint": "npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "check-prettier lint",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "prettier": "prettier -c --write \"**/*\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write",
      "git add"
    ],
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js"
  },
  "author": "playerljc <playerljc@163.com>",
  "repository": {
    "type": "git",
    "url": "https://gitee.com/playerljc/adherev"
  },
  "bugs": {
    "url": "https://gitee.com/playerljc/ReactPro/issues/new"
  },
  "license": "ISC",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.4.0",
    "@baifendian/adherev": "^1.0.0",
    "ant-design-vue": "^1.7.5",
    "vue": "^2.6.12",
    "vue-router": "^3.5.1",
    "classnames": "^2.2.6",
    "copy-to-clipboard": "^3.3.1",
    "faker": "^5.4.0",
    "font-awesome": "^4.7.0",
    "moment": "^2.29.1",
    "core-js": "^3.6.5"
  },
  "devDependencies": {
    "@babel/runtime": "^7.9.2",
    "@ctsj/buildv": "^2.0.1",
    "babel-plugin-transform-vue-jsx": "^3.7.0",
    "@umijs/fabric": "^2.0.8",
    "babel-plugin-import": "^1.13.3",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-plugin-vue": "^7.9.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.0",
    "prettier": "^2.0.5",
    "stylelint": "^13.7.2",
    "tslint": "^6.1.3",
    "typescript": "^3.8.3"
  }
}
