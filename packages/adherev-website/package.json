{
  "name": "@baifendian/adherev-website",
  "version": "1.0.7",
  "private": true,
  "description": "adherevçš„website",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "startapp": "cross-env environment=dev mode=development ctbuildv startapp --define alias=@,evnVars=true,cssModules=true,static=assets",
    "buildapp": "cross-env environment=dev mode=production ctbuildv buildapp --define alias=@,evnVars=true,cssModules=true,static=assets",
    "buildapp:analysis": "cross-env environment=dev mode=production ctbuildv buildapp --define alias=@,evnVars=true,cssModules=true,analysis=true,static=assets",
    "lint": "npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "check-prettier lint",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "prettier": "prettier -c --write \"**/*\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{vue,js,jsx,tsx,ts,less,md,json}": [
      "prettier --write",
      "git add"
    ],
    "**/*.{vue,js,jsx,ts,tsx}": "npm run lint-staged:js"
  },
  "author": "playerljc <playerljc@163.com>",
  "repository": {
    "type": "git",
    "url": "http://git.baifendian.com/SYYF/adherev"
  },
  "bugs": {
    "url": "http://git.baifendian.com/SYYF/adherev/issues"
  },
  "license": "ISC",
  "dependencies": {
    "@ant-design/icons": "^4.4.0",
    "@baifendian/adherev": "^1.0.7",
    "ant-design-vue": "^1.7.5",
    "classnames": "^2.2.6",
    "copy-to-clipboard": "^3.3.1",
    "core-js": "^3.6.5",
    "faker": "^5.4.0",
    "font-awesome": "^4.7.0",
    "highlight.js": "^9.11.0",
    "marked": "^3.0.4",
    "moment": "^2.29.1",
    "ol": "^6.5.0",
    "vue": "^2.6.12",
    "vue-highlight.js": "^3.1.0",
    "vue-i18n": "^8.24.4",
    "vue-router": "^3.5.1"
  },
  "devDependencies": {
    "@babel/runtime": "^7.15.4",
    "@ctsj/buildv": "^3.0.3",
    "@umijs/fabric": "^2.8.0",
    "@vue/cli-plugin-babel": "^4.5.15",
    "@vue/eslint-config-standard": "^6.1.0",
    "babel-plugin-import": "^1.13.3",
    "babel-plugin-transform-vue-jsx": "^3.7.0",
    "check-prettier": "^1.0.3",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-markdown": "^2.2.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-vue": "^7.19.1",
    "husky": "^7.0.2",
    "lint-staged": "^11.2.0",
    "prettier": "^2.4.1",
    "stylelint": "^13.13.1",
    "vue-eslint-parser": "^7.11.0"
  }
}
