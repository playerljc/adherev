  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "lib",
    "es",
    "umd"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "startapp:dev": "cross-env mode=development ctbuildv startapp --define alias=@,evnVars=true",
    "buildpackage:cjs": "ctbuildv buildpackagets -c ctbuildv.package.ts.cjs.config.js -o lib",
    "buildpackage:es": "ctbuildv buildpackagets -c ctbuildv.package.ts.esm.config.js -o es",
    "buildpackage": "npm-run-all --parallel buildpackage:cjs buildpackage:es",
    "buildumd": "ctbuildv buildumd --config ctbuildv.config.umd.js --packagename AdherevVueQuillSandBox --define alias=@,evnVars=true",
    "generatorchangelog": "bd changelog/CHANGELOG.html"
  },
  "resolutions": {},
  "dependencies": {
    "@baifendian/adhere-ui-reactquill-sandbox": "^2.0.2",
    "@baifendian/adherev-util": "^2.0.0",
    "classnames": "^2.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-quill": "^2.0.0",
    "vue": "^3.2.45",
    "vue-types": "^3.0.2"
  },
  "devDependencies": {
    "@babel/runtime": "^7.17.2",
    "@ctsj/buildv": "^5.0.4",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.9.5"
  },
  "peerDependencies": {
    "vue": ">=3.0.0 - <4.x"
  },
  "publishConfig": {
    "access": "public"
  }
